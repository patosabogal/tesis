{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Schema for an application array",
  "patternProperties": {
    "^[a-zA-Z]+(?:[A-Z][a-z]*)*$": {
      "type": "object",
      "properties": {
        "precondition": {
          "type": "string"
        },
        "postcondition": {
          "type": "string"
        }
      }
    }
  }
}